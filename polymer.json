{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/home-view.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled",
      "basePath": "/Heroes-app"
    },
    {
      "preset": "es6-bundled",
      "basePath": "/Heroes-app"
    },
    {
      "preset": "es6-unbundled",
      "basePath": "/Heroes-app"
    }
  ]
}
